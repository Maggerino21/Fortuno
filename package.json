{
  "name": "fortuno",
  "version": "0.1.0",
  "private": true,
  "packageManager": "npm@8.11.0",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "turbo run dev",
    "dev:web": "turbo run dev --filter=@fortuno/web", 
    "dev:mobile": "turbo run start --filter=@fortuno/mobile",
    "build": "turbo run build",
    "start": "turbo run start",
    "lint": "turbo run lint"
  },
  "devDependencies": {
    "turbo": "^1.10.0"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/material": "^7.1.0",
    "@mui/system": "^7.1.0"
  }
}
